{
  "version": 2,
  "buildCommand": "pnpm build & node ./dis/src/main.js",
  "outputDirectory": "dist",
  "env": {
    "DATABASE_URL": "file:../db/data.db"
  },
  "functions": {
    "src/main.js": {
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": "db/data.db"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}