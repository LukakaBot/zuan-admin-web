{
  "version": 2,
  "buildCommand": "pnpm build & node ./dis/src/main.js",
  "outputDirectory": "dist",
  "env": {
    "DATABASE_URL": "file:../db/data.db"
  },
  "builds": [
    {
      "src": "dist/src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}